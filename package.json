    "bullmq": "^5.66.5",
    "crawlee": "^3.15.3",
    "dotenv": "^17.2.3",
    "ioredis": "^5.4.1"
  },
  "devDependencies": {
    "@types/amqplib": "^0.10.8",
    "@types/node": "^22.10.2",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "^5.7.2",
    "vitest": "^4.0.17"
  }
