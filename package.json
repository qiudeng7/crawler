{
  "name": "@qiudeng/crawler-monorepo",
  "version": "1.0.3",
  "private": true,
  "description": "Social crawler service based on rabbitMQ",
  "scripts": {
    "build": "pnpm -r run build",
    "dev:worker": "pnpm --filter @qiudeng/crawler-example run dev:worker",
    "dev:cli": "pnpm --filter @qiudeng/crawler run dev:cli",
    "dev:client": "pnpm --filter @qiudeng/crawler-example run dev:client",
    "worker": "pnpm --filter @qiudeng/crawler-example run worker",
    "cli": "pnpm --filter @qiudeng/crawler run cli",
    "client": "pnpm --filter @qiudeng/crawler-example run client",
    "clean": "pnpm -r run clean && rm -rf node_modules"
  },
  "keywords": [
    "crawler",
    "rabbitmq",
    "crawlee",
    "douyin"
  ],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.25.0",
  "devDependencies": {
    "@types/amqplib": "^0.10.8",
    "@types/node": "^22.10.2",
    "tsup": "^8.5.1",
    "tsx": "^4.21.0",
    "typescript": "^5.7.2"
  }
}
