{
  "name": "@qiudeng/crawler-client",
  "version": "1.0.1",
  "description": "Social crawler service based on rabbitMQ",
  "main": "./dist/src/rabbitmq/client.js",
  "exports": {
    ".": {
      "types": "./dist/src/rabbitmq/client.d.ts",
      "import": "./dist/src/rabbitmq/client.js",
      "default": "./dist/src/rabbitmq/client.js"
    }
  },
  "type": "module",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev:worker": "tsx example/worker.ts",
